<div class="author">
    <h1 class="author__heading">Hi, I am Azamat</h1>
    <p class="author__body">Full stack web developer</p>
</div>

<img src="/img/computer.webp" alt="My photo" class="hero-image" width="400" height="400">

<section class="articles">
<h2 class="articles__heading">Latest posts</h2>

<ul class="article-ul">

    <% data.forEach(post => { %>
     
        <li>
            <a href="#">
                <span><%= post.title %></span>
                <span class="article-list__date"><%= post.createdAt %></span>
            </a>
        </li>
        
        <% }) %>

</ul>

<a href="#" class="pagination">&lt; View Older Posts</a>

</section>